<resources>

    <declare-styleable name="video_play">
        <attr name="line_color" format="color" />
        <attr name="bg_line_color" format="color" />
        <attr name="line_width" format="integer" />
        <attr name="bg_line_width" format="integer" />
    </declare-styleable>

    <declare-styleable name="download">
        <attr name="download_line_color" format="color" />
        <attr name="download_bg_line_color" format="color" />
        <attr name="download_text_color" format="color" />
        <attr name="download_line_width" format="integer" />
        <attr name="download_bg_line_width" format="integer" />
        <attr name="download_text_size" format="integer" />
    </declare-styleable>


    <declare-styleable name="scroll">
        <attr name="scroll_line_color" format="color" />
        <attr name="scroll_line_width" format="integer" />
        <attr name="scroll_bg_line_color" format="color" />
        <attr name="scroll_bg_line_width" format="integer" />
        <attr name="scroll_is_selected" format="boolean" />
        <attr name="scroll_view_translation_x" format="integer" />
    </declare-styleable>

    <declare-styleable name="search">
        <attr name="search_line_color" format="color" />
        <attr name="search_line_width" format="integer" />
        <attr name="search_dot_size" format="integer" />
    </declare-styleable>


</resources>